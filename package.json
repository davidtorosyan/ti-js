{
  "name": "ti-js",
  "version": "0.4.16",
  "description": "TI-Basic interpreter written in JavaScript.",
  "main": "dist/node/ti.js",
  "jsdelivr": "dist/web/ti.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "test": "tap --no-coverage-report",
    "start": "webpack-dev-server --host localhost --config webpack.dev.js",
    "lint": "eslint .",
    "bump": "npm version patch",
    "bt": "npm run build && npm test",
    "push": "npm run bt && npm run lint && npm publish",
    "cov": "tap --reporter=spec",
    "next": "npm run bump && npm run push && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidtorosyan/ti-js.git"
  },
  "keywords": [],
  "author": "David Torosyan <davidtorosyan.git@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidtorosyan/ti-js/issues"
  },
  "homepage": "https://github.com/davidtorosyan/ti-js",
  "devDependencies": {
    "@webpack-cli/serve": "^1.7.0",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "loader-utils": "^3.2.0",
    "peggy": "^2.0.1",
    "peggy-loader": "^0.5.6",
    "raw-loader": "^4.0.2",
    "tap": "^16.3.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {}
}
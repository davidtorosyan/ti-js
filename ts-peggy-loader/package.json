{
  "name": "@davidtorosyan/ts-peggy-loader",
  "version": "0.31.16",
  "description": "Peggy loader for webpack",
  "keywords": [
    "peggy",
    "loader",
    "webpack",
    "typescript"
  ],
  "author": "David Torosyan <davidtorosyan.git@gmail.com>",
  "homepage": "https://github.com/davidtorosyan/ti-js/ts-peggy-loader",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidtorosyan/ti-js.git",
    "directory": "ts-peggy-loader"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "run-s .:build:clean .:build:esm .:build:cjs",
    ".:build:clean": "rimraf dist",
    ".:build:esm": "tsc -p tsconfig.json",
    ".:build:cjs": "tsc -p tsconfig-cjs.json",
    "lint": "lint",
    "lint-and-fix": "lint-and-fix",
    "lint-staged": "lint-staged",
    "bump-next": "bump-next"
  },
  "bugs": {
    "url": "https://github.com/davidtorosyan/ti-js/issues"
  },
  "peerDependencies": {
    "peggy": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "@rocket.chat/eslint-config-alt": "~0.31.16",
    "@rocket.chat/prettier-config": "~0.31.16",
    "@types/node": "~15.14.9",
    "eslint": "~8.8.0",
    "lint-staged": "~12.3.3",
    "npm-run-all": "^4.1.5",
    "peggy": "^2.0.1",
    "prettier": "~2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "~27.1.3",
    "ts-pegjs": "^2.1.0",
    "typescript": "~4.3.5",
    "webpack": "~5.68.0"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  }
